class Solution{
    public static int doUnion(int a[], int n, int b[], int m) 
    {
        HashSet<Integer> s = new HashSet<Integer>();
        
        for(int i : a){
            s.add(i);
        }
        for(int i : b){
            s.add(i);
        }
        return s.size();
    }
}
